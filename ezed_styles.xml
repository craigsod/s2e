<Styles>
	<Style name="Text" element="span">
		<Attribute name="class" value="bodytext"/>
	</Style>
	<Style name="Text " element="span">
		<Attribute name="class" value="bodytext"/>
	</Style>
	<Style name="Header" element="span">
		<Attribute name="class" value="HEADER"/>
	</Style>
	<Style name="Header XLG" element="span">
		<Attribute name="class" value="HEADERXLG"/>
	</Style>
	<Style name="Header XXLG" element="span">
		<Attribute name="class" value="HEADERXXLG"/>
	</Style>
		<Style name="Header Orange" element="span">
		<Attribute name="class" value="HEADERorange"/>
	</Style>
</Styles>